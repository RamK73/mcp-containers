            "cacheDirectories": [
              "/root/.cache/uv"
            ],
            "paths": [
              "/opt/venv/bin"
            ]
          },
          "setup": {
            "nixPkgs": [
              "python3",
              "gcc"
            ],
            "nixLibs": [
              "zlib",
